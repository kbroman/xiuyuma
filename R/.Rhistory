f <- function(z) {
ch <- as.character(z)
ch <- gsub('[\\.0\\-]', '', ch)
firstdig <- as.numeric(substr(ch, 1, 1))
table( factor(firstdig, levels=1:9) )
}
f(23)
f(c(23,34,45,56,78,89))
